{
  "name": "@confidentsecurity/confsec",
  "version": "0.1.2",
  "description": "JavaScript/TypeScript SDK for CONFSEC - secure and anonymous AI inference",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist/confsec.node": "./dist/confsec.node"
  },
  "files": [
    "dist",
    "!dist/**/*.node",
    "binding.gyp",
    "native/src",
    "scripts",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:native && tsup",
    "build:native": "node-gyp rebuild",
    "dev": "tsup --watch",
    "test": "jest --testPathIgnorePatterns '^.*-e2e\\.test\\.ts$'",
    "test:e2e": "jest --testPathPattern '^.*-e2e\\.test\\.ts$'",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "preinstall": "node scripts/download-libconfsec.js",
    "install": "node-gyp rebuild",
    "postinstall": "node scripts/copy-native.js",
    "prepublishOnly": "npm run lint && npm run typecheck && npm run test && npm run build"
  },
  "keywords": [
    "confsec",
    "ai",
    "inference",
    "security",
    "privacy",
    "openai",
    "sdk"
  ],
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/confidentsecurity/confsec-js.git"
  },
  "bugs": {
    "url": "https://github.com/confidentsecurity/confsec-js/issues"
  },
  "homepage": "https://github.com/confidentsecurity/confsec-js",
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "node-gyp": "^10.0.1",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "openai": "^4.20.1"
  },
  "engines": {
    "node": ">=20"
  },
  "libconfsecVersion": "0.1.5"
}
